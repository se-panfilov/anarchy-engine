{
  "name": "anarchy-legal",
  "author": "S. Panfilov",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "sideEffects": false,
  "license": "MIT",
  "scripts": {
    "before:release": "npm run before:release:check && npm run generate:third-party-list && npm run generate:licenses",
    "before:release:check": "npm run ts:check && npm run prettier:check && npm run lint",
    "lint": "eslint ./src",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "ts:check": "tsc --noEmit",
    "ts:check:watch": "tsc --watch --noEmit"
  },
  "dependencies": {
    "@formatjs/intl": "3.1.6",
    "rxjs": "7.8.2"
  }
}
