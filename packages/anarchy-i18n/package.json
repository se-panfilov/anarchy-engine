{
  "name": "anarchy-i18n",
  "author": "S. Panfilov",
  "private": true,
  "version": "1.1.2",
  "type": "module",
  "sideEffects": false,
  "license": "MIT",
  "description": "Anarchy-I18n â€“ a library for internationalization in Anarchy Engine.",
  "keywords": [
    "anarchy",
    "anarchy-engine",
    "i18n",
    "anarchy-i18n",
    "internationalization",
    "localization",
    "language"
  ],
  "files": [
    "./legal/**",
    "CHANGELOG.md",
    "NOTICE.md",
    "LICENSE",
    "README.md",
    "dist/"
  ],
  "scripts": {
    "before:release": "npm run before:release:check",
    "before:release:check": "npm run ts:check && npm run prettier:check && npm run lint",
    "generate:locales": "node Scripts/GenerateLocales.js --config src/Configs/locales.config.jsonc --out-const src/Constants/Locales.gen.ts --out-models src/Models/TLocales.gen.ts",
    "lint": "eslint ./src",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "ts:check": "tsc --noEmit",
    "ts:check:watch": "tsc --watch --noEmit"
  },
  "dependencies": {
    "@formatjs/intl": "3.1.6",
    "rxjs": "7.8.2"
  }
}
