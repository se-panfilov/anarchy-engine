      "@Showcases/Menu/*": ["packages/showcases-menu/src/*"],
      "@Showcases/Menu": ["packages/showcases-menu/src/index.ts"]
    },
    "typeRoots": ["./node_modules/@types", "./node_modules", "../../node_modules/@types", "../../node_modules", "./src/Engine/Types", "./types"]
