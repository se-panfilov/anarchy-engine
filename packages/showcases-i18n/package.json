{
  "name": "showcases-i18n",
  "author": "S. Panfilov",
  "private": true,
  "version": "1.4.1",
  "type": "module",
  "sideEffects": false,
  "license": "MIT",
  "description": "Anarchy Showcases i18n â€“ translations library for Anarchy Engine Showcases.",
  "keywords": [
    "anarchy",
    "showcases",
    "anarchy-showcases",
    "showcases-i18n"
  ],
  "files": [
    "./legal/**",
    "CHANGELOG.md",
    "NOTICE.md",
    "LICENSE",
    "README.md",
    "dist/"
  ],
  "scripts": {
    "before:release": "npm run sort:i18n && npm run before:release:check",
    "before:release:check": "npm run ts:check && npm run check:circular-dependencies && npm run lint",
    "check:circular-dependencies": "npx dpdm -T --no-warning --no-tree --exit-code circular:1 ./src/index.ts",
    "generate:legal": "npm run generate:legal:3rd-party-licenses && npm run generate:legal:notice && npm run generate:legal:files && npm run generate:legal:sbom",
    "generate:legal:3rd-party-licenses": "npm run -w anarchy-legal generate:licenses -- --workspace packages/showcases-i18n --out ../../packages/showcases-i18n/legal/THIRD_PARTY_LICENSES.md",
    "generate:legal:files": "npm run -w anarchy-legal generate:files -- --workspace packages/showcases-i18n --out ../../packages/showcases-i18n",
    "generate:legal:notice": "npm run -w anarchy-legal generate:notice -- --audit --strict --workspace packages/showcases-i18n --source ../../packages/showcases-i18n/legal/THIRD_PARTY_LICENSES.md --out ../../packages/showcases-i18n/legal/NOTICE.md",
    "generate:legal:sbom": "npm sbom --sbom-format=cyclonedx --package-lock-only --omit=dev > ./compliance/sbom/sbom.cdx.json",
    "lint": "eslint ./src",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "sanitize-assets": "node ../../packages/anarchy-shared/ScriptUtils/SanitizeAssets.js ./src/assets",
    "sort:i18n": "sort-json src/i18n/locales/*.json",
    "ts:check": "tsc --noEmit",
    "ts:check:watch": "tsc --watch --noEmit",
    "unit": "vitest run",
    "unit:coverage": "vitest run --coverage",
    "unit:coverage:ui": "vitest --ui",
    "unit:watch": "vitest"
  },
  "dependencies": {
    "anarchy-engine": "*",
    "anarchy-i18n": "*",
    "anarchy-shared": "*",
    "sort-json": "2.0.1",
    "vue-i18n": "11.1.12"
  }
}
