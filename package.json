{
  "name": "Hanami",
  "author": "S. Panfilov",
  "private": true,
  "version": "1.21.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint ./src --ext .js,.ts",
    "lint:fix": "eslint ./src --ext .js,.ts --fix",
    "prettier": "prettier --write .",
    "generate:schemas": "node ./utils/JsonUtils.js",
    "ts:check": "tsc --noEmit",
    "ts:check:watch": "tsc --watch --noEmit",
    "unit": "vitest run",
    "unit:watch": "vitest",
    "unit:coverage": "vitest run --coverage",
    "unit:coverage:ui": "vitest --ui"
  },
  "dependencies": {
    "@rwh/keystrokes": "1.5.4",
    "ajv": "8.12.0",
    "animejs": "3.2.2",
    "lil-gui": "0.19.2",
    "nanoid": "5.0.6",
    "rxjs": "7.8.1",
    "sanitize.css": "13.0.0",
    "three": "0.162.0"
  },
  "devDependencies": {
    "@types/animejs": "3.1.12",
    "@types/node": "20.8.10",
    "@types/three": "0.162.0",
    "@typescript-eslint/eslint-plugin": "7.1.1",
    "@typescript-eslint/parser": "7.1.1",
    "@vitest/coverage-v8": "1.3.1",
    "@vitest/ui": "1.3.1",
    "eslint": "8.57.0",
    "eslint-plugin-functional": "6.0.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-simple-import-sort": "12.0.0",
    "eslint-plugin-sort-export-all": "1.4.1",
    "husky": "4.3.8",
    "jsdom": "24.0.0",
    "prettier": "3.2.5",
    "pretty-quick": "4.0.0",
    "ts-json-schema-generator": "1.5.0",
    "typescript": "5.4.2",
    "vite": "5.1.5",
    "vitest": "1.3.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
