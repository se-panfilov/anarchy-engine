{
  "name": "anarchy_engine",
  "author": "S. Panfilov",
  "private": true,
  "version": "3.2.2",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "rimraf ./dist tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint ./src --ext .js,.ts",
    "lint:fix": "eslint ./src --ext .js,.ts --fix",
    "prettier": "prettier --write .",
    "prettier:staged": "pretty-quick --staged",
    "generate:schemas": "node ./utils/JsonUtils.js",
    "generate:schemas:minify": "node ./utils/JsonUtils.js --minify",
    "generate:licanses-report": "npm install -g license-report && license-report --only=prod --output=markdown > 3RD_PARTY_LICENSES.md",
    "ts:check": "tsc --noEmit",
    "ts:check:watch": "tsc --watch --noEmit",
    "unit": "vitest run",
    "unit:watch": "vitest",
    "unit:coverage": "vitest run --coverage",
    "unit:coverage:ui": "vitest --ui"
  },
  "dependencies": {
    "@dimforge/rapier3d": "0.13.1",
    "@rwh/keystrokes": "1.5.6",
    "ajv": "8.16.0",
    "animejs": "3.2.2",
    "date-fns": "3.6.0",
    "decimal.js": "10.4.3",
    "lil-gui": "0.19.2",
    "nanoid": "5.0.7",
    "rxjs": "7.8.1",
    "sanitize.css": "13.0.0",
    "three": "0.165.0",
    "ts-key-enum": "3.0.12"
  },
  "devDependencies": {
    "@dimforge/rapier3d-compat": "0.13.1",
    "@types/animejs": "3.1.12",
    "@types/date-fns": "2.6.0",
    "@types/node": "20.12.12",
    "@types/three": "0.165.0",
    "@typescript-eslint/eslint-plugin": "7.12.0",
    "@typescript-eslint/parser": "7.12.0",
    "@vitest/coverage-v8": "1.6.0",
    "@vitest/ui": "1.6.0",
    "eslint": "8.57.0",
    "eslint-plugin-functional": "6.5.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-simple-import-sort": "12.1.0",
    "eslint-plugin-sort-export-all": "1.4.1",
    "husky": "4.3.8",
    "jsdom": "24.1.0",
    "prettier": "3.3.1",
    "pretty-quick": "4.0.0",
    "rimraf": "5.0.7",
    "ts-json-schema-generator": "2.3.0",
    "typescript": "5.4.5",
    "vite": "5.3.1",
    "vite-plugin-top-level-await": "1.4.1",
    "vite-plugin-wasm": "3.3.0",
    "vitest": "1.6.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
