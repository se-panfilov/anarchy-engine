{
  "name": "anarchy_monorepo",
  "author": "S. Panfilov",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "license": "MIT",
  "scripts": {
    "clean:all": "rimraf ./dist ./packages/*/dist ./apps/*/dist ./node_modules ./packages/*/node_modules ./apps/*/node_modules ./package-lock.json",
    "clean:reinstall": "npm run clean:all && npm install && git add ./package-lock.json",
    "prettier:changed": "git diff --name-only --diff-filter=ACMRTUXB HEAD | grep -E '\\.(ts|tsx|js|json|css|md)$' | xargs prettier --write",
    "pretty-quick": "pretty-quick --staged"
  },
  "devDependencies": {
    "@eslint/js": "9.29.0",
    "@types/node": "24.0.4",
    "@typescript-eslint/eslint-plugin": "8.35.0",
    "@typescript-eslint/parser": "8.35.0",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "dotenv": "16.5.0",
    "eslint": "9.29.0",
    "eslint-plugin-functional": "9.0.2",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-sort-export-all": "2.1.0",
    "eslint-plugin-spellcheck": "0.0.20",
    "jsdom": "26.1.0",
    "lefthook": "1.11.16",
    "madge": "8.0.0",
    "prettier": "3.6.0",
    "pretty-quick": "4.2.2",
    "rimraf": "6.0.1",
    "rollup-plugin-visualizer": "6.0.3",
    "typescript": "5.8.3",
    "typescript-eslint": "8.35.0",
    "valibot": "1.1.0",
    "vite": "6.3.5",
    "vite-plugin-dts": "4.5.4",
    "vite-plugin-wasm": "3.4.1",
    "vitest": "3.2.4"
  }
}
