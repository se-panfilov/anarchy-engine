{
  "name": "anarchy_engine_showcases",
  "author": "S. Panfilov",
  "private": true,
  "version": "18.0.0",
  "type": "module",
  "license": "MIT",
  "files": [
    "dist",
    "src",
    "LICENSE",
    "README.md",
    "DISCLAIMER.md",
    "THIRD_PARTY_LICENSES.md",
    "THIRD_PARTY_LICENSES_FULL.txt"
  ],
  "scripts": {
    "start:dev": "vite --mode dev",
    "start:e2e": "vite --mode ../e2e --port 5073 --logLevel error",
    "build": "rimraf ./dist tsc && vite build",
    "before:release:check": "npm run ts:check && npm run prettier:check && npm run check:ts-morph && npm run lint && npm run unit:coverage && npm run e2e:with-clean",
    "before:release": "npm run clean:reinstall && npm run before:release:check && npm run generate:license-report && npm run generate:all-licenses-list",
    "preview": "vite preview",
    "lint": "eslint ../../src",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier --write ../..",
    "prettier:check": "prettier --check ../..",
    "prettier:staged": "pretty-quick --staged",
    "prettier:changed": "git diff --name-only --diff-filter=ACMRTUXB HEAD | grep -E '\\.(ts|tsx|js|json|css|md)$' | xargs prettier --write",
    "generate:schemas": "node ../shared/src/utils/JsonUtils.js",
    "generate:schemas:minify": "node ../shared/src/utils/JsonUtils.js --minify",
    "generate:license-report": "npm install -g license-report && license-report --only=prod --output=markdown --department.value='Game Engine' --relatedTo.value='Runtime dependencies' > THIRD_PARTY_LICENSES.md",
    "generate:all-licenses-list": "yarn licenses generate-disclaimer > THIRD_PARTY_LICENSES_FULL.txt",
    "ts:check": "tsc --noEmit",
    "ts:check:watch": "tsc --watch --noEmit",
    "e2e": "playwright test",
    "e2e:debug": "PWDEBUG=1 npx playwright test",
    "e2e:debug:ui": "npx playwright test --ui",
    "e2e:with-clean": "playwright test && npm run e2e:clean-metadate",
    "e2e:headed": "npm run e2e -- --headed",
    "e2e:update": "npm run e2e -- --update-snapshots",
    "e2e:show-report": "playwright show-report ../../reports/e2e/playwright-report",
    "e2e:clean-metadate": "node ../shared/src/utils/PostprocessScreenshots.js",
    "sanitize-assets": "node ../shared/src/utils/SanitizeAssets.js",
    "unit": "vitest run",
    "unit:watch": "vitest",
    "unit:coverage": "vitest run --coverage",
    "unit:coverage:ui": "vitest --ui",
    "clean:all": "rimraf ./dist ./node_modules ./package-lock.json",
    "clean:reinstall": "npm run clean:all && npm install && git add ./package-lock.json",
    "check:no-spread-brand": "node ../../configs/TsMorph/check-no-spread-brand.js ",
    "check:ts-morph": "npm run check:no-spread-brand",
    "check:ts-morph:fix": "npm run check:no-spread-brand -- --fix"
  },
  "dependencies": {
    "@dimforge/rapier3d": "0.17.3",
    "@rwh/keystrokes": "1.5.6",
    "ajv": "8.17.1",
    "date-fns": "4.1.0",
    "lil-gui": "0.20.0",
    "lodash-es": "4.17.21",
    "nanoid": "5.1.5",
    "rbush": "4.0.1",
    "rxjs": "7.8.2",
    "sanitize.css": "13.0.0",
    "stats.js": "0.17.0",
    "three": "0.177.0",
    "three-mesh-bvh": "0.9.1",
    "three-viewport-gizmo": "2.2.0",
    "ts-key-enum": "3.0.13",
    "typescript-fsm": "1.6.0",
    "valibot": "1.1.0"
  },
  "devDependencies": {
    "@dimforge/rapier3d-compat": "0.17.3",
    "@eslint/js": "9.29.0",
    "@gltf-transform/cli": "4.2.0",
    "@playwright/test": "1.53.1",
    "@types/howler": "2.2.12",
    "@types/lodash-es": "4.17.12",
    "@types/node": "24.0.4",
    "@types/rbush": "4.0.0",
    "@types/three": "0.177.0",
    "@typescript-eslint/eslint-plugin": "8.35.0",
    "@typescript-eslint/parser": "8.35.0",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "dotenv": "16.5.0",
    "eslint": "9.29.0",
    "eslint-plugin-functional": "9.0.2",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-sort-export-all": "2.1.0",
    "eslint-plugin-spellcheck": "0.0.20",
    "fast-glob": "3.3.3",
    "jsdom": "26.1.0",
    "playwright": "1.53.1",
    "prettier": "3.6.0",
    "pretty-quick": "4.2.2",
    "rimraf": "6.0.1",
    "sharp": "0.34.2",
    "svgo": "4.0.0",
    "ts-json-schema-generator": "2.4.0",
    "ts-morph": "26.0.0",
    "typescript": "5.8.3",
    "typescript-eslint": "8.35.0",
    "vite": "6.3.5",
    "vite-plugin-wasm": "3.4.1",
    "vitest": "3.2.4"
  }
}
