{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "paths": {
      "@/*": ["apps/showcases/src/*"],
      "@Public/*": ["apps/showcases/public/*"],
      "@Engine/*": ["packages/engine/src/*"],
      "@Engine": ["packages/engine/src/index.ts"],
      "@Shared/*": ["packages/shared/src/*"],
      "@Shared": ["packages/shared/src/index.ts"],
      "@ShowcasesShared/*": ["packages/showcases-shared/src/*"],
      "@ShowcasesShared": ["packages/showcases-shared/src/index.ts"],
      "@E2E/*": ["apps/e2e/src/*"],
      "@E2E": ["apps/e2e/src/index.ts"],
      "showcases_menu/src/main": ["apps/showcases/types/showcases_menu-shim"],
      "platform:api": ["apps/showcases/src/Platform/Drivers/web"]
    }
  },
  "include": ["src", "vite.config.ts", "types", "dist-desktop", "dist-web", "dist-mobile", "dist"]
}
