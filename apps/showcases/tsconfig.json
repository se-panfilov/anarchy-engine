{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "paths": {
      "@/*": ["apps/showcases/src/*"],
      "@Public/*": ["apps/showcases/public/*"],
      "@Engine/*": ["packages/engine/src/*"],
      "@Engine": ["packages/engine/src/index.ts"],
      "@Shared/*": ["packages/shared/src/*"],
      "@Shared": ["packages/shared/src/index.ts"],
      "@ShowcasesShared/*": ["packages/showcase-shared/src/*"],
      "@ShowcasesShared": ["packages/showcase-shared/src/index.ts"],
      "@E2E/*": ["apps/e2e/src/*"],
      "@E2E": ["apps/e2e/src/index.ts"],
      "@Menu/*": ["packages/showcases-menu/src/*"],
      "@Menu": ["packages/showcases-menu/src/index.ts"]
    }
  },
  "include": ["src", "vite.config.ts", "types", "dist-desktop", "dist-web", "dist-mobile", "dist"]
}
